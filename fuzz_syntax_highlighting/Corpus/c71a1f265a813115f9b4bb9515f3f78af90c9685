 F20    // Load values from memory
staSRLrt:  LW, R1, 1000(R0)   // R1 = *Mem(1000)
        LW, R2, 1004(R0)   // R2 = *Mem(1004)
        ADD, R3, R0, R1 J,R11<<<<; ::::::::::::::::::::::   // ::::::R3 = R1
        ADD, R4, R0, R2    // R4 = R2
