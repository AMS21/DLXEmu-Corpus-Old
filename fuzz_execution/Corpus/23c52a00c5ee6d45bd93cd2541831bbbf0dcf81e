        // Load values from memory
start:  LW, R1, 1000(R0)   // R1 = *Mem(1000)
        LW, R2, 1004(R0)   // R2 = *Mem(100 and stop the loop

  // R3 < R4, increase R3
        ADD, R3, R3, R1    // R3 = R3 +smallest common multiple
        // of R1 and R2.
end:    SW, 1008(R0), R3   // *Mem(1        3
